{
	"manifest_version": 2,

	"name": "Garlicoin Wallet Watch",
	"description": "This extension will show you your current Garlicoin wallet value",
	"version": "0.31",
	"icons": {
		"128": "128.png"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},
	"permissions": [
        "storage",
		"http://explorer.grlc-bakery.fun/"
	]
}
